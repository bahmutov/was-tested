{
  "name": "was-tested",
  "description": "Code coverage proxy",
  "version": "0.0.0-semantic-release",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": {
    "was-tested": "./index.js"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/was-tested/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "contributors": [],
  "dependencies": {
    "check-more-types": "2.20.2",
    "ecstatic": "1.4.0",
    "http-proxy": "1.8.1",
    "istanbul": "0.4.3",
    "lazy-ass": "1.4.0",
    "optimist": "0.6.1",
    "quote": "0.2.2",
    "rimraf": "2.2.8",
    "update-notifier": "0.2.2"
  },
  "devDependencies": {
    "eslint-rules": "0.2.0",
    "grunt": "0.4.5",
    "grunt-cli": "1.2.0",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-deps-ok": "0.5.2",
    "grunt-eslint": "2.1.0",
    "grunt-help": "0.5.0",
    "grunt-jscs": "1.1.0",
    "grunt-nice-package": "0.9.2",
    "grunt-readme": "0.4.5",
    "matchdep": "0.3.0",
    "pre-git": "3.8.3",
    "semantic-release": "4.3.5",
    "standard": "6.0.8",
    "standard-format": "2.1.1"
  },
  "engines": {
    "node": "> 0.10.*"
  },
  "homepage": "https://github.com/bahmutov/was-tested",
  "keywords": [
    "testing",
    "code",
    "coverage",
    "testing",
    "istanbul",
    "test",
    "proxy"
  ],
  "license": "MIT",
  "main": "index.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/was-tested.git"
  },
  "scripts": {
    "test": "grunt",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "npm run format && npm run lint",
    "lint": "standard --verbose *.js src/*.js",
    "format": "standard-format -w *.js src/*.js",
    "demo-foo-bar": "node index.js --target http://glebbahmutov.com/foo-bar/ --host glebbahmutov.com",
    "demo-8080": "node index.js --target http://localhost:8080"
  }
}
